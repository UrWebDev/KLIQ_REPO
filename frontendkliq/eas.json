{
  "cli": {
    "version": ">= 13.1.1",
    "appVersionSource": "remote"
  },
  "build": {
    "android": {
      "distribution": "internal",
      "developmentClient": true
    },
    "ios": {
      "distribution": "internal",
      "developmentClient": true
    },
    "preview": {
      "distribution": "internal",
      "developmentClient": false
    },
    "production": {
      "autoIncrement": true,
      "developmentClient": false
    },
    "development": {
      "android": {
        "distribution": "internal",
        "developmentClient": true,
        "cache": {
          "disabled": true
        }
      },
      "ios": {
        "distribution": "internal",
        "developmentClient": true,
        "cache": {
          "disabled": true
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}